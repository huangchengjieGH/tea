  /* border-bottom: 1rpx solid rgb(220, 220, 220); */
}

.cancelBtn {
  position: absolute;
  right: 18px;
  top: 8px;
}

.searchfor {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding-left: 10px;
}

.searchhistory {
  display: flex;
