  top: 40rpx;
}

.yearbatch {
  position: relative;
  padding: 15rpx 0;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.batch {
  position: absolute;
  right: 300rpx;
}

.batchValue {
  /* position: absolute;
  right: 170rpx; */
  margin-left: 200rpx;
}

.Technology {
  padding: 15rpx 0;
  position: relative;
  margin-left: 30rpx;
}

.product {
  position: absolute;
  right: 210rpx;
}

.productValue {
  /* position: absolute;
  right: 50rpx; */
  margin-left: 250rpx;
}

.specification {
  /* padding: 20rpx 30rpx; */
  margin-left: 30rpx;
  margin-top: 15rpx;
}

.reference {
  padding: 15rpx 0 20rpx 30rpx;
}

.price {
  color: red;
  font-size: 20px;
  margin-left: 10rpx;
  letter-spacing: 1px;
}

/* pages/leftpop/leftpop.wxss */
.view_show2 {
  display: block;
}
.view_hide2, .modal1 {
  display: none;
}
.popup-wrapper2 {
  background: rgba(0, 0, 0, 0.4);
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  position: fixed;
  overflow: auto;
  margin: 0;
  z-index: 3;
  animation: fadein 0.5s;
}
.popup-left2 {
  width: 250px;
  background-color: rgb(255, 255, 255);
  position: fixed;
  height: 100%;
  bottom: 0;
  top: 0;
  left: 0;
  box-shadow: rgba(0, 0, 0, 0.270588) 0px -2px 20px 0px;
  flex-direction: column;
  z-index: 200;
  animation: moveright 0.4s ease;
  overflow-y: scroll;
}
.weui-flex__item2 {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 35rpx;
}
.weui-cell_access2 {
  color: inherit;
}
.weui-flex {
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cells {
  margin: 0;
  opacity: 0;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  -webkit-transition: 0.3s;
  transition: 0.3s;
}

.weui-cells:before, .weui-cells:after {
  display: none;
}

.weui-cells_show {
  opacity: 1;
  -webkit-transform: translateY(0);
  transform: translateY(0);
}

.weui-cell:before {
  right: 15px;
}

.kind-list {
  padding: 6px 0;
}

.kind-list__item {
  margin: 10px 0;
  background-color: #fff;
  border-radius: 2px;
  overflow: hidden;
}

.kind-list__item:first-child {
  margin-top: 0;
}

.kind-list__item-hd {
  padding: 6px 12px;
  -webkit-transition: opacity 0.3s;
  transition: opacity 0.3s;
  font-size: 16px;
}

.kind-list__item-hd_show {
  opacity: 0.4;
}

.kind-list__item-bd {
  height: 0;
  overflow: hidden;
}

.kind-list__item-bd_show {
  height: auto;
}

.search_y .searchbar {
  background: #f9f9f9;
}

.search_y .searchbar-result {
  opacity: 1;
  top: 52px;
  left: 0;
  right: 0;
  border-bottom: 1rpx solid #e6e6e6;
}
.weui-cell__bd2 {
  /* cc */
  font-size: 16px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  padding: 10rpx;
}
.year {
  padding: 10rpx 20rpx;
  font-size: 18px;
  border-bottom: 1rpx solid rgb(210, 210, 210);
}

.yearlist {
  width: 100%;
  height: 36px;
  align-items: center;
  display: flex;
  padding-left: 43px;
}
