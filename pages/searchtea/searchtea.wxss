/* pages/searchtea/searchtea.wxss */

page {
  background-color: #f0f2f7;
}

.searchCon {
  display: flex;
  flex-direction: column;
}

.search-y {
  /* position: fixed;  */
  top: 0rpx;
  left: 0;
  right: 0;
}

.weui_cell {
  padding: 0 0 7px 0;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cell_bd {
  /* cc */
  font-size: 14px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.search-y .searchbar {
  background: #4eaefe;
}

.search_btn {
  font-size: 16px;
  padding: 8px 0 8px 10px;
}

.search_btn image {
  margin-top: 5px;
  height: 25px;
  width: 23px;
  display: inline-block;
}

.weui_search-bar {
  position: relative;
  padding: 8px 10px;
   padding-right: 18px; 
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #4eaefe; /*#f9f9f9*//* border-top: 1rpx solid #D7D6DC; *//* border-bottom: 1rpx solid #e6e6e6; */
}

.weui-search-bar_form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #fff;
  border: 1rpx solid #e6e6ea;
}

.weui-search-bar_box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui_icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}

.weui-search-bar_input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.weui_icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}

.weui-search-bar_label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9b9b9b;
  background: #fff;
  line-height: 28px;
  /* display: flex;
  justify-content: center;
  align-items: center; */
}

.weui_icon-search {
  margin-right: 8px;
  font-size: inherit;
  /* margin-top: 3px; */
}

.weui-search-bar_text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}

.weui-search-bar_cancel-btn {
  margin-left: 10px;
  color: white;
  white-space: nowrap;
  background-color: #ec820e;
  padding: 5px 8px;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 300;
}

.history {
  background-color: white;
