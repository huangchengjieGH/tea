  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  margin-left: 60rpx;
}

.add {
  background: white;
  padding: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.triangleUp {
  border-left: 5px solid transparent;
  border-right: 5px solid transparent;
  border-bottom: 5px solid #555;
  width: 0;
}

.triangleDown {
  border-left: 5px solid transparent;
  border-right: 5px solid transparent;
  border-top: 5px solid #555;
  width: 0;
}

.textarea {
  border: 1rpx solid rgb(220, 220, 220);
