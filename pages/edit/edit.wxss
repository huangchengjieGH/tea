.tarBar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 50px;
  background-color: white;
  display: flex;
  flex-direction: row;
  font-size: 12px;
  align-items: center;
  color: rgb(51, 51, 51);
}

.publish {
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 10;
}

.confirmcel {
  margin-top: 20px;
